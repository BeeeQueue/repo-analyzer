{
  "name": "repo-analyzer",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "dependencies": {
    "cloc": "2.5.0",
    "commander": "3.0.2",
    "debug": "4.1.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/node": "12.7.12",
    "husky": "3.0.8",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "simple-git": "1.126.0",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.0-dev.20191010"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts"
  }
}
