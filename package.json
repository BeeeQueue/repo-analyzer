{
  "name": "repo-analyzer",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "dependencies": {
    "cloc": "2.5.0",
    "commander": "3.0.2",
    "debug": "4.1.1",
    "lang-map": "0.4.0",
    "log-update": "4.0.0",
    "open": "6.4.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/node": "12.7.12",
    "husky": "3.0.8",
    "lint-staged": "10.1.3",
    "prettier": "2.0.4",
    "simple-git": "1.132.0",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node -T -r tsconfig-paths/register src/index.ts"
  }
}
